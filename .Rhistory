glm.smokers <- glm(Urban.population ~ leader_ideology+ democracy + + Imports.of.goods.and.services + Military.expenditure....of.GDP. +Per.capita.GNI,
family = "quasipoisson", ## we specify the distribution!
data = new)
## ----summaryPoisson------------------------------------------------
summary(glm.smokers)
resid_sq <- residuals(glm.smokers, type = "pearson")^2
fit_var <- lm(resid_sq ~ mu_hat + I(mu_hat^2) - 1)  # No intercept
mu_hat <- fitted(glm.smokers)
fit_var <- lm(resid_sq ~ mu_hat + I(mu_hat^2) - 1)  # No intercept
summary(fit_var)
